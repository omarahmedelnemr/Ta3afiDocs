            "attachedImages":[
                {
                    "name":"Image 1",
                    "link":"asjdhjkasa.png"
                },
                {
                    "name":"Image 2",
                    "link":"asdasda.png"
                }
            ]
        },
        "responseDescription":"Receives 'Done' if every Thing Went Well, or one of the Common Errors"
    },
